  },
  "dependencies": {
    "next": "16.1.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@unocss/cli": "^0.60.0",
    "@unocss/preset-attributify": "^0.60.0",
    "@unocss/preset-icons": "^0.60.0",
    "@unocss/preset-uno": "^0.60.0",
    "@unocss/preset-web-fonts": "^0.60.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "typescript": "^5",
    "unocss": "^0.60.0"
  }
}
